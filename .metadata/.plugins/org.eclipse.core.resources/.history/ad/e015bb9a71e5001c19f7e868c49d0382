		super();
		this.lines = new ArrayList<>();
	}

	public Cart(List<CartLine> lines) {
		super();
		this.lines = lines;
	}

	public List<CartLine> getLines() {
		return lines;
	}

	public void setLines(List<CartLine> lines) {
		this.lines = lines;
	}

	public double getTotal() {
		double res = 0;
