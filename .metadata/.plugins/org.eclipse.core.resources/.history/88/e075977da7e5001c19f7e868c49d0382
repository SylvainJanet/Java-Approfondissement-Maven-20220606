package o03.threads.swing;

public class temp {
	public static void main(String[] args) {
		for (int i = 0; i < 2; i++) {
			System.out.println("Thread3 - " + i);
			try {
				Thread.sleep(3000);
			} catch (InterruptedException e) {
				e.printStackTrace();
			}
		}
		for (int i = 0; i < 20; i++) {
			System.out.println("Thread4 - " + i);
		}
	}
}
